#include <iostream>
#include <queue>
using namespace std;

int main() {
    queue<int> q;
    q.push(10);
    q.push(20);
    q.push(30);

    cout << "Queue after pushing 10, 20, 30:\n";
    cout << "Front element: " << q.front() << endl;
    cout << "Size: " << q.size() << endl;
    q.pop();

    cout << "\nQueue after one pop:\n";
    cout << "Front element: " << q.front() << endl;
    cout << "Size: " << q.size() << endl;

    if (q.empty()) {
        cout << "\nQueue is empty.\n";
    } else {
        cout << "\nQueue is not empty.\n";
    }

    return 0;
}
